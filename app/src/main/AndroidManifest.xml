        <activity
            android:name=".fragments.mYear"
            android:label="@string/title_activity_m_year" >
        </activity>
        <activity
            android:name=".fragments.mMe"
            android:label="@string/title_activity_m_me" >
        </activity>
        <activity
            android:name=".Aboutus"
            android:label="@string/title_activity_aboutus" >
        </activity>
        <activity
            android:name=".mName"
            android:label="@string/title_activity_m_name" >
        </activity>
        <activity
            android:name=".mNote"
            android:label="@string/title_activity_m_note" >
        </activity>
        <activity
            android:name=".mPassword"
            android:label="@string/title_activity_m_password" >
